<% form_for([:admin, @leadership]) do |f| %>
  <%= f.error_messages %>

  <div class="field">
    <%= f.label :name %><br />
    <%= f.text_field :name %>
  </div>
  <div class="field">
    <%= f.label :title %><br />
    <%= f.text_field :title %>
  </div>
  <div class="field">
    <%= f.label :email %><br />
    <%= f.text_field :email %>
  </div>
  <div class="field">
    <%= f.label :content_1 %><br />
    <%= f.text_area :content_1 %>
  </div>
  <div class="field">
    <%= f.label :content_2 %><br />
    <%= f.text_area :content_2 %>
  </div>
  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>
