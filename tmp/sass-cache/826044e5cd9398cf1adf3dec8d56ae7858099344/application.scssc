  padding: 5px;
  vertical-align: top;
  text-align: right;
}

.labelLayout {
  width: 200px;
  margin: 0;
  padding: 5px 10px 5px 3px;
  border: 0;
  vertical-align: middle;
  text-align: left;
}

label {
  font-family: Calibri;
  font-size: 13px;
  color: #333333;
}

.label {
  font-family: Calibri;
  font-size: 13px;
  color: #333333;
}

label.error { 
	display: none;
	font-family: Calibri;
  font-size: 13px;
	float: none; 
	color: red; 
	padding-left: .5em; 
	vertical-align: top;
}

.textboxLayout, .radio_buttonLayout {
  width: 360px;
  height: 100%;
  margin: 0;
  padding: 2px;
  vertical-align: middle;
  text-align: right;
}

.radioButtonLayout {
  width: 355px;
  padding: 0 0 0 15px;
  text-align: left;
}

.textbox {
  font-family: Arial, Helvetica, sans-serif;
  font-size: 15px;
  color: #333333;
}

.validationLayout {
  width: 0;
  margin: 0;
  border: 0;
  padding: 0;
  vertical-align: top;
  text-align: right;
}

.validation {
  font-family: Calibri;
  font-size: 13px;
  font-weight: bold;
  color: #FF0000;
}

.button {
  font-family: Calibri;
  font-size: 13px;
  font-weight: bold;
  margin: 0 0 0 231px;
}

.buttonLayout, .sentLayout {
  width: 65px;
  height: 100%;
  padding: 3px 5px 2px 5px;
  vertical-align: top;
  text-align: center;
  border: 1px inset #808080;
}

#contactContent p.consent {
  font-size: 11px;
}

table.contactTable {
  background-color: #EFEFEF;
}

/* Utility definitions */

.clear {
  clear: both;
  line-height: .01em;
  padding: 0;
  margin: 0;
}

.blue {
  color: #537DB9;
  font-size: 15px;
}

.one_hundredPercent {
  display: block;
  width: 100%;
  height: 100%;
}

#notice {
	border: solid 1px #99cc99;
	background-color: #e2f9e3;
	color: #006600;
	padding: 5px;
	margin-bottom: 5px;
	a {
		&:link {
		  color: #006600;
		  text-decoration: underline;
		}
		&:hover {
		  color: #006600;
		  text-decoration: none;
		}
	}
	.error {
	  border-color: #cc0000;
	  background-color: #eeaaaa;
	  color: #cc0000;
	}
