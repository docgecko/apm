  padding-right: 4px;
  margin-top: -8px;
}

div#sub_nav_container ul li ul li {
  float: left;
  position: relative;
  text-decoration: none;
  margin: 0;
  font-size: 0.9em;
  padding: 1px 0 1px 4px;
}

/* LEFT NAV */

div#left_nav {
