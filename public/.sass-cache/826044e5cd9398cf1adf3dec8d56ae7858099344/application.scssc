  padding: 0;
  width: 964px;
}

div#logo {
  top: 15px;
  left: 25px;
  position: absolute;
  z-index: 1;
  float: left;
}

/* TOP NAV STYLING */

div#top_nav {
  position: relative;
  top: 0;
  left: 0;
  width: 940px;
  font-size: 11px;
  font-weight: normal;
}

div#util_search {
  float: right;
  height: 28px;
  border: 0;
  margin-bottom: 0px;
  background-color: #F0F0F0;
  color: #537DB9;
}

div#nav_bar_left, div#nav_bar_menu, div#nav_bar_right {
  border: 0;
}

div#nav_bar_left, div#nav_bar_menu {
  float: left;
}

div#nav_bar_right {
  float: right;
}

div#nav_bar_menu {
  padding: 0;
  margin: 7px 0 0;
}

.nav_bar_link {
  color: #537DB9;
  text-decoration: none;
  padding: 2px;
  margin-top: 10px;
}

/* GLOBAL NAV STYLING */

div#nav_container {
  position: relative;
  top: 27px;
  left: 0px;
  clear: both;
  margin: 0;
  padding: 10px 0 0 15px;
  float: left;
  font-family: Arial, Helvetica, sans-serif;
  font-size: 11px;
  font-weight: bold;
  text-decoration: none;
  letter-spacing: 1px;
  text-transform: uppercase;
  color: #F0F0F0;
}

div#nav_container ul {
  margin: 0;
  padding: 10px 0px 0;
  list-style: none;
}

div#nav_container li {
  display: inline;
  margin: 0;
  padding: 0;
}

div#nav_container a {
  float: left;
  margin: 0 12px 0 0;
  padding: 0 0 0 6px;
  text-decoration: none;
}

div#nav_container a span {
  float: left;
  color: #F0F0F0;
}

div#nav_container a span {
  padding: 9px 10px 5px 4px;
}

div#nav_container li a:hover,
div#nav_container ul li a:hover {
  background-image: url('../images/nav_menu_left.jpg');
  background-repeat: no-repeat;
}

div#nav_container li a:hover span,
div#nav_container ul li a span:hover {
  background-image: url('../images/nav_menu_right.jpg');
  background-repeat: no-repeat;
  background-position: right top;
}

/* SUB NAV STYLING  */

div#sub_nav_information, div#sub_nav_container, div#sub_nav_home {
  position: relative;
  top: -13px;
  left: 0px;
  height: 32px;
  margin: 0;
  float: left;
  background-color: transparent;
  background-image: url('../images/nav_sub_shaded.jpg');
  background-repeat: repeat-x;
  list-style: none;
  font-size: 13px;
  font-weight: bold;
  padding: 0 0 0 24px;
  width: 100%;
  z-index: 2;
}

div#sub_nav_container ul {
  padding: 0;
  margin: 0;
  list-style: none;
}

div#sub_nav_container li {
  float: left;
