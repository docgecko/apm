  font-size: 10px;
}

div#top_bg, div#top_home {
  position: relative;
  top: 0px;
  left: 0px;
  width: 100%;
  margin: 0;
  padding: 0;
  background-color: #FFFFFF;
  background-image: url('../images/background_header.jpg');
  background-repeat: repeat-x;
}

div#top_bg {
  height: 115px;
}

div#top_home {
  height: 85px;
}

/* Logo and search box */

div#logo_search {
  clear: both;
